          <stringProp name="script">import java.util.Date;
import java.text.SimpleDateFormat;
import org.apache.jmeter.samplers.SampleResult;
String startTime = vars.get(&quot;startTime&quot;);
String timeStamp2 = new SimpleDateFormat(&quot;yyyy-MM-dd_HH-mm-ss&quot;).format(new Date());
String scriptName = ctx.getThreadGroup().getName();

//Write if error 
if (!prev.isSuccessful())
{
FileWriter fWriter = new FileWriter(vars.get(&quot;scriptPath&quot;) + &quot;/&quot; + scriptName + &quot;-full_error_log_&quot; + startTime + &quot;.txt&quot;, true);


BufferedWriter buff = new BufferedWriter(fWriter);
buff.write(&quot;==================== ERROR ENTRY ====================&quot;);
buff.write(System.getProperty(&quot;line.separator&quot;));
buff.write(timeStamp2);
buff.write(System.getProperty(&quot;line.separator&quot;));
buff.write(&quot;Elapsed time (ms): &quot; + ctx.getPreviousResult().getTime());
buff.write(System.getProperty(&quot;line.separator&quot;));
buff.write(&quot;Request:&quot;);
buff.write(System.getProperty(&quot;line.separator&quot;));
buff.write(prev.getSamplerData());
buff.write(System.getProperty(&quot;line.separator&quot;));
buff.write(&quot;Request headers:&quot;);
buff.write(System.getProperty(&quot;line.separator&quot;));
buff.write(prev.getRequestHeaders());
buff.write(System.getProperty(&quot;line.separator&quot;));
buff.write(&quot;Response Code : &quot; + ctx.getPreviousResult().getResponseCode());
buff.write(System.getProperty(&quot;line.separator&quot;));
buff.write(&quot;Response Headers : &quot; + ctx.getPreviousResult().getResponseHeaders());
buff.write(System.getProperty(&quot;line.separator&quot;));
buff.write(&quot;Response Body : &quot;);
buff.write(System.getProperty(&quot;line.separator&quot;));
buff.write(new String(ctx.getPreviousResult().getResponseData()));
buff.write(System.getProperty(&quot;line.separator&quot;));
buff.write(&quot;==================== END OF ENTRY ====================&quot;);
buff.write(System.getProperty(&quot;line.separator&quot;));
buff.write(System.getProperty(&quot;line.separator&quot;));
buff.close();
fWriter.close();
}</stringProp>
